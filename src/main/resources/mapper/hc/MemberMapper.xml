<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="net.onione.data.mapper.hc.MemberMapper">
    <select id="getAllMember" resultType="net.onione.model.dto.AllMemberOutDto">
        SELECT m.member_id
             , m.username
          FROM member m
         WHERE 1=1
    </select>
</mapper>
